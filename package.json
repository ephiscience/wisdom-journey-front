{
	"name": "wisdom-journey-front",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --hmr --configuration=fr",
		"build": "ng build",
		"test": "ng test",
		"test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
		"test:ci:locales": "ng build --configuration lint-locales",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"docker:build": "docker build --pull -t ghcr.io/ephiscience/wisdom-journey/front .",
		"docker:push": "docker build --pull -t ghcr.io/ephiscience/wisdom-journey/front . && docker push ghcr.io/ephiscience/wisdom-journey/front:latest",
		"docker:run": "docker run --rm -p 0.0.0.0:2015:80 -it ghcr.io/ephiscience/wisdom-journey/front",
		"i18n": "ng extract-i18n --output-path src/locale"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^18.2.13",
