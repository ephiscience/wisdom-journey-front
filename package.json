		"@angular/platform-browser-dynamic": "^18.2.13",
		"@angular/router": "^18.2.13",
		"@apollo/client": "3.13.1",
		"@fontsource/chela-one": "^4.5.8",
		"@fontsource/roboto": "^4.5.8",
